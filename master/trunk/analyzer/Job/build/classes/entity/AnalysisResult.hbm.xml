<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>						
    <class name="entity.AnalysisResult" table="analysisresult" catalog="jobresult">
	
      <!-- 主键 -->
        <id name="analysisId" type="java.lang.Integer">
            <column name="analysisId" />
            <generator class="native" />
        </id>
         <property name="city" length="3" not-null="false" />
		 <property name="salary" length="10" not-null="false" />
         <property name="positionName" length="10" not-null="false"/>  
         <property name="peopleNum" length="5" not-null="false"/>   	
         <property name="type" length="3" not-null="false"/>   	 	
   
    </class>
</hibernate-mapping>
